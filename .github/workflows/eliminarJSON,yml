name: Eliminar archivo

on:
  workflow_dispatch:
    # Inputs the workflow accepts.

jobs:
  eliminar-archivo:
    runs-on: ubuntu-latest

    steps:
    - name: Clonar repositorio
      uses: actions/checkout@v2
      
    - name: Eliminar archivo
      run: |
        git rm datos.json
        git commit -m "Eliminado archivo datos.json"
        git push
